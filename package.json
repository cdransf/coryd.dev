    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "rimraf": "^6.0.1",
    "slugify": "^1.6.6",
    "terser": "^5.37.0",
    "truncate-html": "^1.1.2"
  }
